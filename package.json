    "dotenv": "^6.2.0",
    "husky": "^1.3.1",
    "inquirer": "^1.1.2",
    "json-ts": "^1.6.4",
    "prettier": "^1.19.1",
    "pretty-quick": "^1.11.1",
    "rimraf": "^2.7.1",
    "ts-node": "^8.10.2",
    "tslint": "^5.20.1",
    "typedoc": "^0.21.9",
    "typedoc-plugin-markdown": "^3.11.3",
    "typescript": "^4.4.4"
  }
